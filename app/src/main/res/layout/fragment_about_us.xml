<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.fazooz.AboutUs"
    android:clickable="true"
    android:background="#FFFF">




    <!-- TODO: Update blank fragment layout -->




    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/abhi"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/abhishek"
        app:civ_border_color="#232528"
        app:civ_border_width="1dp"
        android:layout_marginTop="69dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/pi" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/pi"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/piyush"
        app:civ_border_color="#232528"
        app:civ_border_width="1dp"
        android:layout_marginTop="43dp"
        android:layout_marginStart="35dp"
        android:layout_below="@+id/ans_abhi"
        android:layout_alignParentStart="true" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/yug"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/yugandhar"
        app:civ_border_color="#232528"
        app:civ_border_width="1dp"
        android:layout_marginEnd="27dp"
        android:layout_alignTop="@+id/pi"
        android:layout_alignParentEnd="true" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/alok_circle"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/alok"
        app:civ_border_color="#232528"
        app:civ_border_width="1dp"
        android:layout_alignTop="@+id/abhi"
        android:layout_alignStart="@+id/yug" />

    <TextView
        android:id="@+id/ans_abhi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/abhi"
        android:layout_below="@+id/abhi"
        android:layout_marginTop="14dp"
        android:text="Abhishek"
        android:textAppearance="@android:style/TextAppearance.Large"
        android:textColor="#000" />

    <TextView
        android:id="@+id/piyush_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Piyush Jain"
        android:textColor="#000000"
        android:textAppearance="@android:style/TextAppearance.Large"
        android:layout_alignBaseline="@+id/ans_yug"
        android:layout_alignBottom="@+id/ans_yug"
        android:layout_alignEnd="@+id/pi" />

    <TextView
        android:id="@+id/ans_yug"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Yugandhar"
        android:textColor="#000000"
        android:textAppearance="@android:style/TextAppearance.Large"
        android:layout_below="@+id/yug"
        android:layout_alignEnd="@+id/yug" />

    <TextView
        android:id="@+id/Alok_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alok Kiran"
        android:textColor="#000000"
        android:textAppearance="@android:style/TextAppearance.Large"
        android:layout_alignBaseline="@+id/ans_abhi"
        android:layout_alignBottom="@+id/ans_abhi"
        android:layout_alignStart="@+id/ans_yug" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="#212223"
        android:text="Contact Us"
        android:textColor="#FFF"/>

</RelativeLayout>
